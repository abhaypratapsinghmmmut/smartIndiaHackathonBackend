{
  "version": 2,
  "builds": [
    {
      "src": "api/app.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "api/routes/**",
          "api/models/**",
          "dist/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/app.js"
    }
  ]
}
